{
  "functions": {
    "src/app/i/[assetType]/[id]/[width]/route.ts": {
      "memory": 512,
      "maxDuration": 10
    },
    "src/app/r/[assetType]/[width]/route.ts": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/raw/:path*",
      "destination": "https://desilog-assets.sivaramp.com/raw/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "https://desilog-assets.sivaramp.com/static/:path*"
    }
  ]
}
