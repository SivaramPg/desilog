{
  "functions": {
    "src/app/i/avatars/[id]": {
      "memory": 256,
      "maxDuration": 3
    },
    "src/app/i/characters/[id]": {
      "memory": 256,
      "maxDuration": 3
    },
    "src/app/i/characters-bw/[id]": {
      "memory": 256,
      "maxDuration": 3
    }
  },
  "redirects": [
    {
      "source": "/raw/:path*",
      "destination": "https://desilog-assets.sivaramp.com/raw/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "https://desilog-assets.sivaramp.com/static/:path*"
    }
  ],
  "rewrites": [
    {
      "source": "/i/avatars/:id/:width",
      "destination": "/i/avatars/:id/?width=:width"
    },
    {
      "source": "/i/avatars/:id/:width",
      "destination": "/i/avatars/:id/?width=:width"
    },
    {
      "source": "/i/characters/:id/:width",
      "destination": "/i/characters/:id/?width=:width"
    },
    {
      "source": "/i/characters-bw/:id/:width",
      "destination": "/i/characters-bw/:id/?width=:width"
    }
  ]
}
